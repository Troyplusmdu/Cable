<%--
Default skin template. The following skins are provided as examples only.

1. Named control skin. The SkinId should be uniquely defined because
   duplicate SkinId's per control type are not allowed in the same theme.

<asp:GridView runat="server" SkinId="gridviewSkin" BackColor="White" >
   <AlternatingRowStyle BackColor="Blue" />
</asp:GridView>

2. Default skin. The SkinId is not defined. Only one default 
   control skin per control type is allowed in the same theme.

<asp:Image runat="server" ImageUrl="~/images/image1.jpg" />
--%>

<asp:GridView runat="server" AutoGenerateColumns="false" PageSize="12" AllowPaging="True" BackColor="White">
    <RowStyle CssClass="dataRow" />
    <SelectedRowStyle CssClass="SelectdataRow" />
    <AlternatingRowStyle CssClass="altRow" />
    <EmptyDataRowStyle CssClass="gridTitle" />
    <HeaderStyle Height="25px" CssClass="GrdHeaderbgClr" />
    <FooterStyle Height="27px" CssClass="HeadataRow" />
    <PagerStyle Height="25px"  CssClass="footer-row allPad" VerticalAlign="Middle" HorizontalAlign="Left"  />
</asp:GridView>

<asp:GridView runat="server" AutoGenerateColumns="false" PageSize="15" GridLines="None" SkinID="GrBigHeader" AllowPaging="True" >
    <RowStyle CssClass="dataRow" />
    <AlternatingRowStyle CssClass="altRow" />
    <EmptyDataRowStyle CssClass="gridTitle" />
    <HeaderStyle Height="30px" CssClass="HeadataRow" />
    <FooterStyle CssClass="footer-row" Height="25px" />
    <PagerStyle Height="25px"  CssClass="footer-row allPad" VerticalAlign="Middle" HorizontalAlign="Left"  />
</asp:GridView>

